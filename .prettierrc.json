{
  "singleQuote": true,
  "semi": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "printWidth": 100,
  "endOfLine": "lf",
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindClassOrder": [
    "w-",
    "h-",
    "m-",
    "p-",
    "text-",
    "bg-",
    "border-",
    "flex-",
    "grid-",
    "order-"
  ]
}
